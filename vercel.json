{
  "version": 2,
  "name": "school-management-system-backend",
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "@NODE_ENV",
    "PORT": "@PORT",
    "MONGODB_URI": "@MONGODB_URI",
    "DATABASE_NAME": "@DATABASE_NAME",
    "API_VERSION": "@API_VERSION",
    "SECRET_KEY": "@SECRET_KEY",
    "SECRET_TOKEN": "@SECRET_TOKEN",
    "ANNOUNCEMENT_COLLECTION_NAME": "@ANNOUNCEMENT_COLLECTION_NAME",
    "ADMIN_COLLECTION_NAME": "@ADMIN_COLLECTION_NAME",
    "ADMINISTRATION_COLLECTION_NAME": "@ADMINISTRATION_COLLECTION_NAME",
    "CATEGORY_COLLECTION_NAME": "@CATEGORY_COLLECTION_NAME",
    "LEVEL_COLLECTION_NAME": "@LEVEL_COLLECTION_NAME",
    "DOWNLOAD_COLLECTION_NAME": "@DOWNLOAD_COLLECTION_NAME",
    "NOTICE_COLLECTION_NAME": "@NOTICE_COLLECTION_NAME",
    "RESULT_COLLECTION_NAME": "@RESULT_COLLECTION_NAME",
    "ROUTINE_COLLECTION_NAME": "@ROUTINE_COLLECTION_NAME",
    "STUDENT_COLLECTION_NAME": "@STUDENT_COLLECTION_NAME",
    "EMAIL_SERVICE": "@EMAIL_SERVICE",
    "EMAIL_SERVICE_USER": "@EMAIL_SERVICE_USER",
    "EMAIL_SERVICE_PASSWORD": "@EMAIL_SERVICE_PASSWORD",
    "EMAIL_SERVICE_DESTINATION_EMAIL": "@EMAIL_SERVICE_DESTINATION_EMAIL"
  }
}
